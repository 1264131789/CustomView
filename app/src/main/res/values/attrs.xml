<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTextView">
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" type="color"/>
        <attr name="backGround" format="color|reference"/>
        <attr name="maxLines" format="integer"/>
        <attr name="inputType" format="enum">
            <enum name="number" value="0"/>
            <enum name="text" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="QQStepView">
        <attr name="qqTextSize" format="dimension"/>
        <attr name="qqTextColor" format="color"/>
        <attr name="outerColor" format="color"/>
        <attr name="innerColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ChangeColorTextView">
        <attr name="changeColor" format="color"/>
        <attr name="originColor" format="color"/>
    </declare-styleable>
</resources>